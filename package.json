{
  "name": "use-worker-promise",
  "version": "1.1.1",
  "description": "A tiny and performant react hook for communicating with Web Workers",
  "keywords": [
    "web worker",
    "react",
    "hooks"
  ],
  "main": "index.mjs",
  "module": "index.mjs",
  "exports": {
    ".": "./index.mjs",
    "./register": "./register/index.mjs"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "prettier": "npx prettier src/*.mts --write",
    "min:useWorkerPromise": "npx terser --compress --mangle --toplevel useWorkerPromise.mjs",
    "min:useWorkerMemo": "npx terser --compress --mangle --toplevel useWorkerMemo.mjs"
  },
  "files": [
    "createWorkerFactory.d.mts",
    "createWorkerFactory.mjs ",
    "index.d.mts ",
    "index.mjs ",
    "useWorkerMemo.d.mts ",
    "useWorkerMemo.mjs ",
    "useWorkerPromise.d.mts ",
    "useWorkerPromise.mjs ",
    "register/index.mjs",
    "register/index.d.mts"
  ],
  "author": "Jan Nicklas",
  "license": "MIT",
  "homepage": "https://github.com/jantimon/useWorkerPromise#readme",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/jantimon/useWorkerPromise.git"
  },
  "dependencies": {
    "promise-worker": "^2.0.1"
  },
  "devDependencies": {
    "@types/react": "18.0.9",
    "react": "18.1.0",
    "terser": "^5.13.1",
    "typescript": "^4.5.4"
  }
}
