{
  "name": "use-worker-promise",
  "version": "1.2.2",
  "description": "A tiny and performant react hook for communicating with Web Workers",
  "keywords": [
    "web worker",
    "react",
    "hooks"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./register": "./register/index.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "parcel build && tsc -p tsconfig.build.json",
    "prettier": "npx prettier src/*.mts --write",
    "min": "npm run useWorkerPromise && npm run min:useWorkerMemo",
    "min:useWorkerPromise": "npx terser --compress --mangle --toplevel useWorkerPromise.mjs",
    "min:useWorkerMemo": "npx terser --compress --mangle --toplevel useWorkerMemo.mjs",
    "size": "npm run size:useWorkerPromise && npm run size:useWorkerMemo",
    "size:useWorkerPromise": "npx terser --compress --mangle --toplevel useWorkerPromise.mjs | npx gzip-size",
    "size:useWorkerMemo": "npx terser --compress --mangle --toplevel useWorkerMemo.mjs | npx gzip-size"
  },
  "files": [
    "dist/",
    "register/"
  ],
  "author": "Jan Nicklas",
  "license": "MIT",
  "homepage": "https://github.com/jantimon/useWorkerPromise#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jantimon/useWorkerPromise.git"
  },
  "dependencies": {
    "promise-worker": "^2.0.1"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.5.0",
    "@parcel/transformer-typescript-types": "^2.5.0",
    "@types/react": "18.0.9",
    "gzip-size-cli": "^5.1.0",
    "parcel": "^2.5.0",
    "react": "18.1.0",
    "terser": "^5.13.1",
    "typescript": "^4.5.4"
  }
}
